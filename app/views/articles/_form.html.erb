<%= form_with(model: article, local: true, multipart: true ) do |form| %>
    <div class="form-group">
      <%= form.text_area :url, placeholder: "記事のURL", rows: "10", class: "form-control" %>
    </div>
    <div class="form-group">
      <%= form.collection_select(:genre_id, Genre.all, :id, :name, {}, { class: "form-control"}) %>
      </div>

      <%= form.submit "SEND" %>
    <% end %>
